{
  "appSettings": {
    "user": "USERNAME",
    "key": "ACCESS_KEY",
    "server": "hub-cloud.browserstack.com/wd/hub"
  },
  "profile": {
    "single": {
      "project": "Ultra",
      "build": "Csharp MStest",
      "name": "Single Test",
      "browserstack.selenium_version": "3.5.2"
    },
    "parallel": {
      "project": "Ultra",
      "build": "Csharp MStest",
      "name": "Parallel Test"
    },
    "local": {
      "project": "Ultra",
      "build": "Csharp MStest",
      "name": "Local Test",
      "browserstack.local": "true",
      "browserstack.selenium_version": "3.5.2"
    }
  },
  "environment": {
    "chrome": {
      "os": "Windows",
      "os_version": "10",
      "browser": "Chrome",
      "browser_version": "latest"

    },
    "firefox": {
      "os": "Windows",
      "os_version": "10",
      "browser": "firefox",
      "browser_version": "latest"

    },
    "ie": {
      "os": "Windows",
      "os_version": "10",
      "browser": "IE",
      "browser_version": 11.0

    },
    "mobile": {
      "os_version": "9.0",
      "device": "Google Pixel 3",
      "real_mobile": "true"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
